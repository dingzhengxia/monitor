    "order_block_interaction": {
      "enabled": true,
      "swing_length": 50,
      "atr_multiplier": 0.1,
      "alert_on_rejection": true,
      "alert_on_breakout": true,
      "exclude_timeframes": []
    },
    "high_funding_rate": {
      "enabled": true,
      "threshold": 0.01,
      "cooldown_mult": 2,
      "scan_top_n": 500,
      "exclude_timeframes": [
        "15m",
        "1h",
        "1d"
      ]
    },
    "trend_channel_breakout": [
