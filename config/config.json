{
  "app_settings": {
    "exchange": "binance",
    "default_market_type": "swap",
    "check_interval_minutes": 5,
    "log_level": "INFO",
    "max_workers": 2
  },
  "market_settings": {
    "timeframes": [
      "1h",
      "4h",
      "1d"
    ],
    "static_symbols": [
      "BTC",
      "ETH",
      "BNB"
    ],
    "dynamic_scan": {
      "enabled": false,
      "top_n_for_signals": 100,
      "primary_quote_currency": "USDT",
      "cross_market_filter": {
        "enabled": true,
        "must_exist_in": [
          "USDC"
        ]
      },
      "exclude_symbols": [
        "USDT",
        "USD1",
        "USDS",
        "USDC",
        "DAI",
        "TUSD",
        "BUSD",
        "FDUSD",
        "PAXG",
        "USDP",
        "GUSD",
        "USDD",
        "EUROC"
      ]
    }
  },
  "strategy_params": {
