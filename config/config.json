    "ema_cross": {
      "enabled": true,
      "period": 120,
      "volume_confirm": true,
      "exempt_static_on_volume": true,
      "volume_multiplier": 2,
      "atr_period": 14,
      "atr_multiplier": 1,
      "exclude_timeframes": []
