      "enabled": true,
      "dynamic_count": {
        "enabled": true,
        "method": "linear_stepped",
        "min_count": 6,
        "max_count": 12,
        "apply_to_rank_n": 100,
        "rank_step_size": 10,
        "default_count": 9
